{
  "name": "coursera",
  "version": "1.0.0",
  "description": "Primer proyecto",
  "main": "index.html",
  "scripts": {
    "dev": "lite-server",
    "start": "concurrently \"npm run watch: scss\" \"npm run dev\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "scss": "node-sass -o css/ css/",
    "watch:scss": "onchange 'css/*.scss' -- npm run scss",
    "clean": "rimraf dist",
    "imagemin": "imagemin imagenes/* --out-dir dist/imagenes",
    "usemin": "usemin index.html -d dist --htmlmin -o dist/index.html && usemin about.html -d dist --htmlmin -o dist/about.html && usemin condition.html -d dist --htmlmin -o dist/condition.html && usemin contact.html -d dist --htmlmin -o dist/contact.html && usemin prices.html -d dist --htmlmin -o dist/prices.html",
    "build": "npm run clean && npm run scss && npm run imagemin && npm run usemin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ale2028/coursera.git"
  },
  "author": "Alejandra Alvarez",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ale2028/coursera/issues"
  },
  "homepage": "https://github.com/ale2028/coursera#readme",
  "devDependencies": {
    "copyfiles": "^2.2.0",
    "cssmin": "^0.4.3",
    "htmlmin": "0.0.7",
    "lite-server": "^2.5.4",
    "onchange": "^6.1.0",
    "rimraf": "^3.0.0",
    "uglifyjs": "^2.4.11",
    "usemin-cli": "^0.5.1"
  },
  "dependencies": {
    "bootstrap": "^4.4.1",
    "concurrently": "^5.0.2",
    "jquery": "^3.4.1",
    "less": "^3.10.3",
    "node-sass": "^4.13.1",
    "open-iconic": "^1.1.1",
    "popper.js": "^1.16.0",
    "sass": "^1.24.5",
    "start": "^5.1.0"
  }
}
